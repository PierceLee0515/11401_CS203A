#include <iostream>
#include <string>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    char ch;
    bool open = true;  // true = 下一個 " 要變成 `` ，false = 下一個 " 要變成 ''

    while (cin.get(ch)) {  // 逐字讀取，直到 EOF
        if (ch == '"') {
            if (open) {
                cout << "``";
                open = false;
            }
            else {
                cout << "''";
                open = true;
            }
        }
        else {
            cout << ch;   // 其他字原樣輸出
        }
    }

    return 0;
}
