#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main() {
    string s;

    while (getline(cin,s)) {  // 逐字讀取，直到 EOF
        string word;
        char c;
        for (int i = 0; i < s.length(); i++) {
            c = s[i];
            if (c != ' ') {
                word.push_back(c);
            }
            else {
                reverse(word.begin(), word.end());
                cout << word;
                word.clear();
                cout << c;
            }
        }
        if (word.empty()!=1) {
            reverse(word.begin(), word.end());
            cout << word;
        }
    }

    return 0;
}
