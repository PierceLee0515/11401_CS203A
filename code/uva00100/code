#include <iostream>
#include<cmath>
using namespace std;

int main() {
    int i, j;
    while (cin >> i >> j) {
        int mx = 0;
        for (int n = min(i, j); n <= max(i, j); n++) {
            int n1 = n, cnt = 1;
            while (n1 != 1) {
                if (n1 % 2 == 0) {
                    n1 /= 2;
                }
                else {
                    n1 = 3 * n1 + 1;
                }
                cnt++;
            }
            mx = max(cnt, mx);
        }
        cout << i << " " << j << " " << mx << "\n";
    }
    return 0;
}
